<div class="container border m-2 p-2">
    <input type="hidden" id="dOrder" value="@ViewBag.orderId" />
    <table id="tblData" class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th>Title</th>
                <th>Author</th>
                <th>Description</th>
                <th>Quantity</th>
                <th>Price</th>
                <th>Amount</th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th colspan="5" style="text-align:right">Total:</th>
                <td></td>
            </tr>
        </tfoot>
    </table>
</div>
@section Scripts{
    <script src="~/js/DetailOrd.js"></script>
}

